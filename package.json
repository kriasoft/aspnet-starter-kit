    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "extend": "^3.0.0",
    "file-loader": "^0.8.5",
    "front-matter": "^2.1.0",
    "highlight.js": "^9.4.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^6.0.5",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "pixrem": "^3.0.1",
    "pleeease-filters": "^3.0.0",
    "postcss": "^5.0.21",
    "postcss-calc": "^5.2.1",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.1",
    "postcss-selector-not": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "style-loader": "^0.13.1",
    "stylelint": "^6.6.0",
    "stylelint-config-standard": "^9.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015-webpack",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single"
    }
  },
  "scripts": {
    "postinstall": "dotnet restore server server.test",
    "eslint": "eslint client client.test tools",
    "stylelint": "stylelint \"client/components/**/*.css\" \"client/routes/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "mocha \"client.test\" --compilers js:babel-register",
    "test:watch": "mocha \"client.test\" --compilers js:babel-register --reporter min --watch",
    "clean": "node tools/clean",
    "build": "node tools/build --production",
    "deploy": "node tools/deploy --production",
    "start": "node tools/start"
  }
}
